<AuList::Item>
  <AuButton
    @icon="add"
    @iconAlignment="left"
    @skin="link"
    {{on 'click' (fn this.insertDate true)}}>
    {{t "date-plugin.insert.date"}}
  </AuButton>
</AuList::Item>
<AuList::Item>
  <AuButton
    @icon="add"
    @iconAlignment="left"
    @skin="link"
    {{on 'click' (fn this.insertDate false)}}>
    {{t "date-plugin.insert.datetime"}}
  </AuButton>
</AuList::Item>
