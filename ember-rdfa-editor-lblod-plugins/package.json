      "./models/template.js": "./dist/_app_/models/template.js",
      "./services/standard-template-plugin.js": "./dist/_app_/services/standard-template-plugin.js"
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./addon-main.js": "./addon-main.js"
  }
